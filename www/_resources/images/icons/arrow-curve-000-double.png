			function fallback() { // wuala not running
				document.location.href = "/en/launch?wualaPath=" + pageUrl + getKey('&');					
			}

			function openInWuala() {
				launch(baseUrl[1]);
			}

			function openFile(index) {
				launch(baseUrl[1] + items[index][1]);
			}

			function getSlideshowFooter(index) {
				index = parseInt(index) + parseInt(imagesOffset);
				var footer = '<a href="http://content.wuala.com/contents' + baseUrl[0] + items[index][0] + '?dl=1' + getKey('&') + '" style="text-decoration: underline;">';

				footer = footer + '</a>';
				footer = footer + '<div style="margin-top: 5px;">';
				footer = footer + '<a id="openInWuala"';
				footer = footer + ' rev="' + index + '"';
				footer = footer + ' href="http://wuala.com' + baseUrl[0] + items[index][1] + getKey('?') + '"';
				footer = footer + ' style="text-decoration: underline;">';
				footer = footer + '<img src="/img/wuala_icon.png" alt="" style="margin-bottom: -2px" /> Open in Wuala';
				footer = footer + '</a>';
				footer = footer + '</div>';
				return footer;
			}

			function getKey(sep) {
				if (key == "") {
					return "";
				} else {
					return sep + "key=" + key;		
				}
			}
				
			function launch(url) {
				pageUrl = url;
									if(wualaHttpLoader) {
